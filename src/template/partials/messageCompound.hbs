{{#unless (isDefined value)}}
{{> messageSegmentName segment=this}}: {
    name: "{{> messageSegmentName segment=this}}",
    type: "{{type}}",
    isOptional: {{isOptional}},
    isRepeatable: {{isRepeatable}},
    {{#if parentCompound}}
    parentCompound: "{{> messageSegmentName segment=parentCompound}}",
	{{/if}}
} as Hl7IMessageCompound,
{{/unless}}