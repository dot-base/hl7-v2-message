{{> messageSegmentName segment=this}}: {
    type: "{{type}}",
    isOptional: {{isOptional}},
    isRepeatable: {{isRepeatable}},
    {{#if parentCompound}}
    parentCompound: this.segments.{{> messageSegmentName segment=parentCompound}},
	{{/if}}
	{{#if (isDefined value)}}
	value: []
	{{/if}}
} as {{#if (isDefined value)}}Hl7IMessageSegment<{{type}}_Segment>{{else}}Hl7IMessageCompound{{/if}},
