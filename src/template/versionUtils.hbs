import Hl7Message from '../../model/Hl7Message';
import Hl7Segment from '../../model/Hl7Segment';

{{#each (values versionTypes.messages)}}
{{> versionUtilsTypeImport name=name type="Message"}}
{{/each}}

{{#each (values versionTypes.segments)}}
{{> versionUtilsTypeImport name=type type="Segment"}}
{{/each}}
export function getMessage(messageType: string): Hl7Message {
    {{#each (values versionTypes.messages)}}
    {{> versionUtilsInitType name=name type="Message"}}
    {{/each}}
    throw new Error(`Unknown message type ${messageType}`);
}

export function getSegment(segmentType: string): Hl7Segment {
    {{#each (values versionTypes.segments)}}
    {{> versionUtilsInitType name=type type="Segment"}}
    {{/each}}
    throw new Error(`Unknown segment type ${segmentType}`);
}
